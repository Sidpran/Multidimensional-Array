#include <iostream>
using namespace std;

int main() {
    int mat[2][3];

    cout << "Enter elements of 2x3 matrix:\n";
    for (int i = 0; i < 2; i++)
        for (int j = 0; j < 3; j++)
            cin >> mat[i][j];

    cout << "Comparing elements of Row 1 and Row 2:\n";
    for (int j = 0; j < 3; j++) {
        if (mat[0][j] == mat[1][j])
            cout << "Element " << j << ": Equal\n";
        else
            cout << "Element " << j << ": Not Equal\n";
    }
    return 0;
}
